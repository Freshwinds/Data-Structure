#include"wtgraph.cpp"
void menu()
{
    cout<<"***********menu**********"<<endl;
    cout<<"+v add vertex"<<endl;
    cout<<"=v w wt  add edge between v and w, weight:wt"<<endl;
    cout<<"?v   look up vertex"<<endl;
    cout<<"#v w look up edge between v and w, input weight"<<endl;
    cout<<"-v  delete vertex"<<endl;
    cout<<"!v w delete edge between v and w"<<endl;
    cout<<"E judge whether the graph is empty or not"<<endl;
    cout<<"F judge whether the graph is full or not"<<endl;
    cout<<"C clear graph"<<endl;
    cout<<"Q exit"<<endl;

}
